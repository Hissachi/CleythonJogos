{
  "name": "cleython",
  "version": "1.0.0",
  "scripts": {
    "dev": "webpack --watch --mode=development",
    "build": "webpack --mode=production",
    "typecheck": "tsc --noEmit",
    "tailwind:dev": "tailwindcss -i ./src/scss/main.scss -o ./dist/css/main.css --watch",
    "tailwind:build": "tailwindcss -i ./src/scss/main.scss -o ./dist/css/main.css --minify"
  },
  "devDependencies": {
    "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
    "@types/jquery": "^3.5.32",
    "autoprefixer": "^10.4.21",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "jquery": "^3.7.1",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-scss": "^4.0.9",
    "sass": "^1.90.0",
    "sass-loader": "^13.3.3",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.2",
    "webpack-cli": "^6.0.1"
  }
}
